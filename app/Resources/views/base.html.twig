<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <!--     Fonts and icons     -->
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
    {% block stylesheets %}
        <!-- Material Kit CSS -->
        <link href="{{ asset('assets/css/material-kit.css?v=2.2.0') }}" rel="stylesheet" />
        <link href="{{ asset('assets/css/myStyle.css') }}" rel="stylesheet" />
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
{% block header %}
    <nav class="navbar navbar-color-on-scroll navbar-transparent fixed-top navbar-expand-lg" color-on-scroll="100">
        <div class="container">
            <div class="navbar-translate">
                <a class="navbar-brand" href="https://demos.creative-tim.com/material-kit-pro/index.html">
                    <i class="material-icons icon-success">public</i>
                    Refugees.org</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="navbar-toggler-icon"></span>
                    <span class="navbar-toggler-icon"></span>
                    <span class="navbar-toggler-icon"></span>
                </button>

            </div>
            <div class="collapse navbar-collapse">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        <a href="{{ path ('home_homepage1') }}" class="nav-link">
                            <i class="material-icons">home</i> Home
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#" class="nav-link">
                            <i class="material-icons">phone</i> Contact
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#" class="nav-link">
                            <i class="material-icons">live_help</i> About us
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#" class="nav-link">
                            <i class="material-icons">payment</i> Donate
                        </a>
                    </li>
                    {% if is_granted('ROLE_USER') %}
                        <li class="nav-item">
                            <a href="{{ path('volunteer_association_profile') }}" class="nav-link">
                                <i class="material-icons"></i> Profile
                            </a>
                        </li>
                        <li class="button-container nav-item iframe-extern">
                            <a href="{{ path('fos_user_security_logout') }}" target="_blank" class="btn  btn-primary   btn-round btn-block">
                                <i class="material-icons">person</i> Logout
                                <div class="ripple-container"></div></a>
                        </li>
                    {% else %}
                        <li class="nav-item">
                            <a  href="{{ path('fos_user_registration_register') }}" class="nav-link" >
                                <i class="material-icons">emoji_people</i> Volunteer
                                <div class="ripple-container"></div></a>

                        </li>
                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                <i class="material-icons"></i> Associations
                            </a>
                        </li>
                        <li class="button-container nav-item iframe-extern">
                            <a href="{{ path('fos_user_security_login') }}" target="_blank" class="btn  btn-primary   btn-round btn-block">
                                <i class="material-icons">person</i> Login
                                <div class="ripple-container"></div></a>
                        </li>
                    {% endif %}

                </ul>
            </div>
        </div>
    </nav>
{% endblock %}

{% block content %}
{% endblock %}

{% block login %}
{% endblock %}


